{
    "name": "rpahlevy/slim-starter-bootstrap4",
    "description": "A Slim Framework skeleton application for rapid development",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "http://github.com/rpahlevy/slim-starter-bootstrap4",
    "license": "MIT",
    "authors": [
        {
            "name": "Rio Pahlevy",
            "email": "rio.pahlevy@gmail.com",
            "homepage": "http://github.com/rpahlevy"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "slim/slim": "^3.1",
        "slim/twig-view": "^2.5",
        "vlucas/phpdotenv": "^2.4",
        "slim/flash": "^0.4.0",
        "monolog/monolog": "^1.17"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8081 -t public"
    }
}